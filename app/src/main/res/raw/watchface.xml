<WatchFace
    clipShape="CIRCLE"
    height="450"
    width="450" >
    <Metadata
        key="CLOCK_TYPE"
        value="ANALOG" />
    <Metadata
        key="PREVIEW_TIME"
        value="10:08:32" />
    <UserConfigurations>
        <ColorConfiguration
            id="contentColor"
            displayName="content"
            screenReaderText="content"
            defaultValue="color_red" >
            <ColorOption
                id="color_red"
                displayName="color_red"
                screenReaderText="color_red"
                colors="#E57373" />
            <ColorOption
                id="color_pink"
                displayName="color_pink"
                screenReaderText="color_pink"
                colors="#F06292" />
            <ColorOption
                id="color_green"
                displayName="color_green"
                screenReaderText="color_green"
                colors="#66BB6A" />
        </ColorConfiguration>
    </UserConfigurations>
    <Scene>
        <PartDraw
            x="0"
            y="0"
            width="450"
            height="450"
            angle="0.0"
            pivotX="0.5"
            pivotY="0.5"
            alpha="255"
            renderMode="ALL" >
            <Arc
                centerX="225.0"
                centerY="225.0"
                height="440.0"
                width="440.0"
                startAngle="0.0"
                endAngle="359.999"
                direction="CLOCKWISE" >
                <Stroke
                    cap="ROUND"
                    color="#4d408bf4"
                    thickness="10" />
            </Arc>
            <Arc
                centerX="225.0"
                centerY="225.0"
                height="440.0"
                width="440.0"
                startAngle="0.0"
                direction="CLOCKWISE" >
                <Transform
                    target="endAngle"
                    value="6 * [SECOND_MILLISECOND]" />
                <Stroke
                    cap="ROUND"
                    color="[CONFIGURATION.contentColor.0]"
                    thickness="10" />
            </Arc>
        </PartDraw>
    </Scene>
</WatchFace>
